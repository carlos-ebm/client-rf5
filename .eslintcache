, Footer } = Layout;\r\n\r\n  //esto debe descomentarse para hacer uso de los tokens\r\n  const { user, isLoading } = useAuth();\r\n\r\n  //esto es provisorio hasta el uso de tokens\r\n  //const user = \"juan\";\r\n\r\n  //console.log(getUserApi(localStorage.getItem(ACCESS_TOKEN), localStorage.getItem(ADMIN_ID)));\r\n  \r\n\r\n  if (!user && !isLoading) {\r\n    return (\r\n      <>\r\n        <Route path=\"/admin/login\" component={AdminSignIn} />\r\n        <Redirect to=\"/admin/login\" />\r\n      </>\r\n    );\r\n  }\r\n\r\n  if (user && !isLoading) {\r\n    return (\r\n      //Establece la estructura del Layout.\r\n      <Layout>\r\n        <Layout className=\"layout-admin\">\r\n          <Header className=\"layout-admin__bar-top\">\r\n            <BarTop />\r\n          </Header>\r\n          <div className=\"layout-admin__header\">\r\n            <MenuTop />\r\n          </div>\r\n          <Content className=\"layout-admin__content\">\r\n            <LoadRoutes routes={routes} />\r\n          </Content>\r\n        </Layout>\r\n        <Layout className=\"layout-admin\">\r\n          <Footer className=\"layout-admin__footer\">\r\n            Radio F5 - Copyright 2021 | By Bortex{\" \"}\r\n          </Footer>\r\n        </Layout>\r\n      </Layout>\r\n    );\r\n  }\r\n  return null;\r\n}\r\n\r\n//Observacin: Aplica destructuring directamente a un objeto que pasa por parametro.\r\n/*\r\n    La funcin LoadRouters() genera una ciclo y retorna todas las rutas encontradas \r\n    en una coleccin de tipo mapa.\r\n*/\r\nfunction LoadRoutes({ routes }) {\r\n  return routes.map((route, index) => (\r\n    <Route\r\n      key={index}\r\n      path={route.path}\r\n      exact={route.exact}\r\n      component={route.component}\r\n    />\r\n  ));\r\n}\r\n",["1090","1091"],"C:\\Users\\Carlos\\Desktop\\GIT\\rf5\\client\\src\\components\\Visitor\\MenuTop\\index.js",[],"C:\\Users\\Carlos\\Desktop\\GIT\\rf5\\client\\src\\components\\Admin\\EditProfileForm\\index.js",[],"C:\\User